   oDbf := DbfServer( ( cFileName ), ( cFileName ) ):New( ( cFileName ), ( cFileName ), ( cVia ), "personal", ( cPath ) )

      oDbf:AddField( "cSerExp", "C", 01, 0,,,,, "Serie", .F.,, .F., {} )
