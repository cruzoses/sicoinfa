UTILITY STATIC function TDetCabeceraPlantillaXML_OpenFiles(lExclusive) ; local Self AS CLASS TDetCabeceraPlantillaXML := QSelf() AS CLASS TDetCabeceraPlantillaXML

   local lOpen             := .T.
   local oError
   local oBlock

   IIF( lExclusive == nil, lExclusive := .F., ) ;

   oBlock                  := ErrorBlock( {| oError | ApoloBreak( oError ) } )
   BEGIN SEQUENCE

      if Empty( ::oDbf )
         ::oDbf            := ::DefineFiles()
      end

      ::oDbf:Activate( .F., !( lExclusive ) )

      ::bOnPreSave         := {|| ::SaveDetails() }
      ::bOnPreSaveDetail   := {|| ::SaveDetails() }

      ::bOnPreAppend       := {|| ::LoadValues() }
      ::bOnPreEdit         := {|| ::LoadValues() }
      ::bOnPostSave        := {|| ::SaveValues() }

   RECOVER USING oError

      msgStop( "Imposible abrir las bases de datos detalle de fidelización." + Chr(13)+Chr(10) + ErrorMessage( oError ) )

      ::CloseFiles()

      lOpen                := .F.

   end

   ErrorBlock( oBlock )

RETURN ( lOpen )



UTILITY STATIC function TDetCabeceraPlantillaXML_CloseFiles() ; local Self AS CLASS TDetCabeceraPlantillaXML := QSelf() AS CLASS TDetCabeceraPlantillaXML

   if !Empty( ::oDbf )
      ::oDbf:end()
   end

   ::oDbf               := nil

RETURN .T.



