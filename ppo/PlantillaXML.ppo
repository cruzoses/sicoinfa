UTILITY STATIC function TPlantillaXML_LoadConfig( cText) ; local Self AS CLASS TPlantillaXML := QSelf() AS CLASS TPlantillaXML

   local nPos
   local cData
   local cField

   if ::oSav:Seek( Upper( cText ) )

      cData          := Rtrim( ::oSav:cData )

      while ( nPos := At( ",", cData ) ) > 0

         if nPos <> 0

            cField   := SubStr( cData, 1, nPos - 1 )
            cField   := Rtrim( cField )

            if ::oDbf:SeekInOrd( cField, "cCodigo" )
               aAdd( ::aPlantilla, { ::oDbf:cCodigo, ::oDbf:cDescrip, ::oDbf:cTipo } )
            end

            ++nPos

            cData    := SubStr( cData, nPos )

         end

      end

      ::cNameConfig  := Padr( cText, 100 )

   else

      MsgStop( "Configuración " + cText + " no encontrada." )

   end

Return ( Self )



UTILITY STATIC function TPlantillaXML_lLoadButtons() ; local Self AS CLASS TPlantillaXML := QSelf() AS CLASS TPlantillaXML

   local cText
   local bAction

   ::oBtnImportacion:DeleteBranches()

   ::oSav:GoTop()
   while !::oSav:eof()

      cText       := by( Rtrim( ::oSav:cText ) )
      bAction     := ::bAction( cText )

      ::oWndBrw:NewAt( "Flash_", , , bAction, cText, , , , , ::oBtnImportacion )

      ::oSav:Skip()

   end

   ::oWndBrw:Refresh()

return nil



UTILITY STATIC function TPlantillaXML_bAction( cText) ; local Self AS CLASS TPlantillaXML := QSelf() AS CLASS TPlantillaXML

   local bGen     := {|| ::Importacion( cText ) }

Return ( bGen )









