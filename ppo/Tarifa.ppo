      ( dbfTarPreT )->( dbCloseArea() )
      dbfTarPreT  := nil
   else
      msgStop( "Imposible abrir en modo exclusivo la tabla de tarifas" )
   end

   dbUseArea( .T., ( cDriver() ), ( cPath + "TARPREL.DBF" ), ( cCheckArea( "TARPREL", @dbfTarPreL ) ), if(.F. .OR. .T., !.T., NIL), .F.,, )
