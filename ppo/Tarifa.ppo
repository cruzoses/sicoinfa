STATIC FUNCTION CloseFiles()

   IIF( !Empty( dbfTarPreT ), ( dbfTarPreT )->( dbCloseArea() ), )
   IIF( !Empty( dbfTarPreL ), ( dbfTarPreL )->( dbCloseArea() ), )
   IIF( !Empty( dbfTarPreS ), ( dbfTarPreS )->( dbCloseArea() ), )
   IIF( !Empty( dbfArticulo), ( dbfArticulo)->( dbCloseArea() ), )
   IIF( !Empty( dbfAgentes ), ( dbfAgentes )->( dbCloseArea() ), )
   IIF( !Empty( dbfDivisa  ), ( dbfDivisa  )->( dbCloseArea() ), )
   IIF( !Empty( dbfFamilia ), ( dbfFamilia )->( dbCloseArea() ), )
   IIF( !Empty( dbfPro     ), ( dbfPro     )->( dbCloseArea() ), )
   IIF( !Empty( dbfProL    ), ( dbfProL    )->( dbCloseArea() ), )

    dbfTarPreT    := NIL
    dbfTarPreL     := NIL
    dbfTarPreS    := NIL
    dbfArticulo    := NIL
    dbfAgentes    := NIL
   dbfDivisa   := NIL
   dbfFamilia  := NIL
   oBandera    := NIL
   dbfPro      := NIL
   dbfProL     := NIL

   if oWndBrw  <> nil
      oWndBrw  := nil
   end

Return ( .T. )



FUNCTION mkTarifa( cPath, oMeter, lAppend, cPathOld )

   IIF( cPath == nil, cPath := cPatArt(), ) ;

   CreateFiles( oMeter, cPath, lAppend, cPathOld )
   Reindex( oMeter, cPath )

   if lAppend .AND. lIsDir( cPathOld )
      AppDbf( cPathOld, cPath, "TARPRET" )
      AppDbf( cPathOld, cPath, "TARPREL" )
      AppDbf( cPathOld, cPath, "TARPRES" )
   end

RETURN NIL



STATIC FUNCTION CreateFiles( oMeter, cPath )

   IIF( cPath == nil, cPath := cPatArt(), ) ;

   if !lExistTable( cPath + "TARPRET.DBF" )
      dbCreate( cPath + "TARPRET.DBF", aSqlStruct( aItmTar() ), cDriver() )
   end

   if !lExistTable( cPath + "TARPREL.DBF" )
      dbCreate( cPath + "TARPREL.DBF", aSqlStruct( aDetalle ), cDriver() )
   end

   if !lExistTable( cPath + "TARPRES.DBF" )
      dbCreate( cPath + "TARPRES.DBF", aSqlStruct( aDeta2 ), cDriver() )
   end

RETURN NIL



FUNCTION rxTarifa( cPath, oMeter )

RETURN Reindex( oMeter, cPath )



STATIC FUNCTION Reindex( oMeter, cPath )

   IIF( cPath == nil, cPath := cPatArt(), ) ;



   IF !lExistTable( cPath + "TARPRET.DBF" ) .OR.  !lExistTable( cPath + "TARPREL.DBF" ) .OR.  !lExistTable( cPath + "TARPRES.DBF" )

        CreateFiles()

    end

   fEraseIndex( cPath + "TARPRET.CDX" )
   fEraseIndex( cPath + "TARPREL.CDX" )
   fEraseIndex( cPath + "TARPRES.CDX" )

   dbUseArea( .T., ( cDriver() ), ( cPath + "TARPRET.DBF" ), ( cCheckArea( "TARPRET", @dbfTarPreT ) ), if(.F. .OR. .T., !.T., NIL), .F.,, )
   if !( dbfTarPreT )->( neterr() )
      ( dbfTarPreT )->( __dbPack() )

      ( dbfTarPreT )->( ordCondSet("!Deleted()", {||!Deleted()}  ) )
      ( dbfTarPreT )->( ordCreate( cPath + "TARPRET", "CCODTAR", "Field->CCODTAR", {|| Field->CCODTAR } ) )

      ( dbfTarPreT )->( ordCondSet("!Deleted()", {||!Deleted()}  ) )
