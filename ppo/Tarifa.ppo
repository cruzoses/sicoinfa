    cTagPreL                := (dbfTarPreL)->( OrdSetFocus( 1 ) )
    cTagPreS                := (dbfTarPreS)->( OrdSetFocus( 1 ) )

    WinAppRec( Nil, bEdit2, dbfTarPreL, nil, nil, cCodArt )

